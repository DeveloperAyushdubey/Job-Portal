@model IEnumerable<Mvc_Projetc.ApplyApplication>
<div class="container">
    <div class="row">
        <div class="col-md-3 mt-5">
            @Html.Partial("_Sidebar")
        </div>
        <div class="col-md-9 mt-5">

            <h1>Application Index</h1>
            <hr />
           
            <table class="table">
                <tr>
                    <th>
                        @Html.DisplayNameFor(model => model.name)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.email)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.education)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.jid)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Experience)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.status)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.resume)
                    </th>
                    <th></th>
                </tr>

                @foreach (var item in Model)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.name)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.email)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.education)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.jid)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Experience)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.status)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.resume)
                        </td>
                        <td>
                            @Html.ActionLink("Edit", "Edit", new { id = item.id }) |
                            @Html.ActionLink("Details", "Details", new { id = item.id }) |
                            @Html.ActionLink("Delete", "Delete", new { id = item.id })
                        </td>
                    </tr>
                }

            </table>

        </div>
    </div>
   
</div>